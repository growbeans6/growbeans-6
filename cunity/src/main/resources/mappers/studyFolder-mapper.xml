<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.growbeans.cunity.studyFolder.mapper.StudyFolderMapper">
	<resultMap type="StudyFolder" id="studyFolderResultSet">
		<id property="folderNo" column="FOLDER_NO"/>
		<result property="studyNo" column="STUDY_NO"/>
		<result property="folderName" column="FOLDER_NAME"/>
		<result property="parentFolderNo" column="PARENT_FOLDER_NO"/>
	</resultMap>
	<insert id="insertStudyfolder" parameterType="StudyFolder">
		insert into studyFolder
		values ( #{folderNo}, #{studyNo})
	</insert>
</mapper>
